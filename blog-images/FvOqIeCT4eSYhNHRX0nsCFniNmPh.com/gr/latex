    function accept_loop() {
    if(!window.__engine) {
      setTimeout(accept_loop, 1000);
      return;
    }
    window.__engine.option.cardSelect._itemDefinitions.file.accept = window.__engine.option.cardSelect._itemDefinitions.file.accept.filter(function(v) {
      return !/\w\.\w/.test(v);
    });
